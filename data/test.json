{
  "groups": {
    "number": "[0-9]",
    "lower": "[a-z]",
    "upper": "[A-Z]"
  },
  "terminals": ["(", ")", "*", "+"],
  "rules": {
    "I": [["lower"], ["I", "lower"], ["I", "number"]],
    "F": [["I"], ["(", "E", ")"]],
    "T": [["F"], ["T", "*", "F"]],
    "E": [["T"], ["E", "+", "T"]]
  }
}
